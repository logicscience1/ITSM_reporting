{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "rg-ims-emea-devops-reporting-data-factory"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "[concat(parameters('factoryName'), '/serviceNow_Task_4_TK')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"description": "rest query to serviceNow for Task",
				"linkedServiceName": {
					"referenceName": "serviceNOW_prod_REST",
					"type": "LinkedServiceReference"
				},
				"annotations": [],
				"type": "RestResource",
				"typeProperties": {
					"relativeUrl": "table/metric_instance?sysparm_exclude_reference_link=true&sysparm_suppress_pagination_header=true&sysparm_limit=2&sysparm_fields=start,end,sys_created_on,table,value,id,sys_id,dv_definition,duration,field_value&sysparm_query=endONToday@javascript:gs.beginningOfToday()@javascript:gs.endOfToday()"
				},
				"schema": []
			},
			"dependsOn": []
		}
	]
}