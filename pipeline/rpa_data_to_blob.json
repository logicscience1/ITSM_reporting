{
	"name": "rpa_data_to_blob",
	"properties": {
		"description": "moves csv files from file storage to blob storage for powerBI ingest",
		"activities": [
			{
				"name": "Copy_b3k",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [
					{
						"name": "Source",
						"value": "example.csv"
					},
					{
						"name": "Destination",
						"value": "rpa-data/example.csv"
					}
				],
				"typeProperties": {
					"source": {
						"type": "DelimitedTextSource",
						"storeSettings": {
							"type": "AzureFileStorageReadSettings",
							"maxConcurrentConnections": 2,
							"recursive": false
						},
						"formatSettings": {
							"type": "DelimitedTextReadSettings",
							"skipLineCount": 0
						}
					},
					"sink": {
						"type": "DelimitedTextSink",
						"storeSettings": {
							"type": "AzureBlobStorageWriteSettings",
							"maxConcurrentConnections": 1
						},
						"formatSettings": {
							"type": "DelimitedTextWriteSettings",
							"quoteAllText": true,
							"fileExtension": ".txt"
						}
					},
					"enableStaging": false,
					"validateDataConsistency": false
				},
				"inputs": [
					{
						"referenceName": "RPA_source_emailProcessorBot",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "RPA_destination_emailProcessorBot",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": [],
		"lastPublishTime": "2021-03-14T08:20:42Z"
	},
	"type": "Microsoft.DataFactory/factories/pipelines"
}